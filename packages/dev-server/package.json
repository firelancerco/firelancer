    "dependencies": {
        "@firelancerco/admin-ui-plugin": "^1.0.4",
        "@firelancerco/asset-server-plugin": "^1.0.4",
        "@firelancerco/core": "^1.0.4"
    },
    "devDependencies": {
        "ts-node": "^10.9.2",
        "typescript": "^5.8.2"
    }
}
